{
  "name": "bikemizer-solid",
  "version": "2.0.0",
  "description": "Sistema de customização de bicicletas com arquitetura SOLID e React",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "start": "node server.js",
    "docker:build": "docker build -t bikemizer-app .",
    "docker:run": "docker run -p 3000:3000 bikemizer-app",
    "docker:up": "docker-compose up --build",
    "k8s:start": "./scripts/deploy-minikube.sh start",
    "k8s:stop": "./scripts/deploy-minikube.sh stop",
    "k8s:restart": "./scripts/deploy-minikube.sh restart",
    "k8s:status": "./scripts/deploy-minikube.sh status",
    "k8s:logs": "./scripts/deploy-minikube.sh logs",
    "k8s:dashboard": "./scripts/deploy-minikube.sh dashboard",
    "k8s:tunnel": "./scripts/deploy-minikube.sh tunnel",
    "security:test": "snyk test",
    "security:test:docker": "snyk container test bikemizer-app:latest",
    "security:monitor": "snyk monitor",
    "security:monitor:docker": "snyk container monitor bikemizer-app:latest",
    "security:fix": "snyk wizard",
    "security:audit": "npm audit && snyk test",
    "security:scan": "./scripts/security-scan.sh test",
    "security:scan:docker": "./scripts/security-scan.sh docker",
    "security:scan:full": "./scripts/security-scan.sh audit",
    "security:report": "./scripts/security-scan.sh report"
  },
  "keywords": ["bicicleta", "customização", "react", "typescript", "solid", "clean-architecture"],
  "author": "BikeMizer",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "zustand": "^4.4.0",
    "axios": "^1.6.0",
    "class-validator": "^0.14.0",
    "react-hook-form": "^7.48.0",
    "react-query": "^3.39.0",
    "framer-motion": "^10.16.0",
    "lucide-react": "^0.294.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0",
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.23",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0",
    "vite": "^4.4.0",
    "vitest": "^0.34.0",
    "snyk": "^1.1248.0"
  }
} 